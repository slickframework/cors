{
    "name": "slick/cors",
    "description": "A CORS module for Slick Framework",
    "type": "library",
    "version": "v0.3.0",
    "require": {
        "php": ">=8.2",
        "slick/configuration": "^2.2@dev",
        "slick/error-handler": ">=v0.4.0",
        "slick/http": ">=v3.0.3",
        "slick/json-api": "^1.0@dev",
        "slick/module-api": ">=v0.4.0",
        "symfony/routing": "7.2.x-dev"
    },
    "keywords": [
        "slick",
        "CORS",
        "slick-modules"
    ],
    "require-dev": {
        "phpmd/phpmd": "dev-master",
        "phpunit/phpunit": "11.3.x-dev",
        "phpstan/phpstan": "2.0.x-dev",
        "phpspec/prophecy-phpunit": "2.x-dev",
        "phpspec/prophecy": "1.x-dev",
        "squizlabs/php_codesniffer": "4.0.x-dev"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Slick\\Cors\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Slick\\Cors\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Filipe Silva",
            "email": "silvam.filipe@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-main": "0.3-dev",
            "dev-develop": "0.4.x-dev"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src",
        "check-code": "phpmd --ignore-errors-on-exit src html build/phpmd.xml > build/phpmd.html && phpstan analyze"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
